package sistema;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class testdos extends DataProducto {

	@Test
	void testInsertarProducto() {
		NegocioProducto np=new NegocioProducto();
		np.setId("1");
		np.setNombre("Cocalo Wspuma");
		np.setPrecio("15");
		np.setProveedor("Cocacola");
		assertTrue(np.insertarProducto());;
	}

}
